{% extends "FOSUserBundle::layout.html.twig" %}

{% trans_default_domain 'FOSUserBundle' %}

{% block fos_user_content %}



    <form novalidate="novalidate" action="{{ path("fos_user_security_check") }}" method="post" class="separate-sections fill-up validatable">
        <input type="hidden" name="_csrf_token" value="{{ csrf_token }}"/>


        <div class="padded" style="margin-top: 30px;">

            {% if error %}
                <div class="alert alert-block">
                    <button type="button" class="close" data-dismiss="alert">Ã—</button>
                    <h4>Warning!</h4>

                    <div>{{ error|trans }}</div>
                </div>
            {% endif %}

            <div class="login box" style="margin-top: 30px;">


                <div class="box-header">
                    <span class="title">{{ "layout.login"|trans }}</span>
                </div>
                <div class="box-content padded">
                    <div class="input-group addon-left">
            <span class="input-group-addon" href="#">
              <i class="icon-user"></i>
            </span>


                        {# <label for="username">{{ 'security.login.username'|trans }}</label>#}
                        <input type="text" id="_username" name="_username" value="{{ last_username }}"
                               required="required" autocomplete="off"
                               style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-position: right center; cursor: auto;"
                               placeholder="{{ 'security.login.username'|trans }}" class="validate[required]"/>
                    </div>
                    <p></p>
                    {# <label for="password">{{ 'security.login.password'|trans }}</label> #}
                    <div class="input-group addon-left">
            <span class="input-group-addon" href="#">
              <i class="icon-key"></i>
            </span>
                        <input type="password" id="password" name="_password" required="required" autocomplete="off"
                               style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-position: right center;"
                               placeholder="{{ 'security.login.password'|trans }}" class="validate[required]"/>
                    </div>

                    <p></p>
                    <div>
                        <input class="icheck" checked type="checkbox" id="remember_me" name="_remember_me" value="on">
                        <label for="remember_me">{{ 'security.login.remember_me'|trans }}</label>
                    </div>
                    {#<div>
                        <input type="checkbox" id="remember_me" name="_remember_me" value="on"/>
                        <label for="remember_me">{{ 'security.login.remember_me'|trans }}</label>
                    </div>#}
                    <p></p>
                    <input type="submit" id="_submit" name="_submit" value="{{ 'security.login.submit'|trans }}"
                           class="btn btn-blue btn-block"/>


                    {#<div>
                        <a href="sign_up.html">
                            Don't have an account? <strong>Sign Up</strong>
                        </a>
                    </div>#}
                </div>
            </div>

        </div>
    </form>
{% endblock fos_user_content %}

